{
  "name": "blogWebapp",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
		"build-css":"node-sass src/styles -o src/transStyles",
		"watch-css":"npm run build-css && node-sass src/styles -o src/transStyles --watch",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
		"start":"npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
